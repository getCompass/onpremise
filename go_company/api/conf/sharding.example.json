{
	"mysql": {
		"ssl": false,
		"max_connections": 5,
		"db_list": [
			"company_conversation",
			"company_thread",
			"company_data"
		]
	},
	"rabbit": {
		"local": {
			"host": "${RABBIT_HOST}",
			"port": "${RABBIT_PORT}",
			"user": "${RABBIT_USER}",
			"pass": "${RABBIT_PASS}"
		},
		"service": {
			"host": "${RABBIT_HOST}",
			"port": "${RABBIT_PORT}",
			"user": "${RABBIT_USER}",
			"pass": "${RABBIT_PASS}"
		}
	},
	"go": {
		"sender": {
			"host": "${GO_SENDER_HOST}",
			"grpc_port": "${GRPC_PORT}",
			"tcp_port": "${TCP_PORT}"
		},
		"company_cache": {
			"host": "${GO_COMPANY_CACHE_HOST}",
			"grpc_port": "${GRPC_PORT}",
			"tcp_port": "${TCP_PORT}"
		}
	}
}
